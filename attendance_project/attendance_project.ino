  }
  delay(2000); 
  oled.setCursor(0, 0);
}
void loop() {
  oled.clearDisplay();
  Serial.println("Enter a reg no: ");
  while (!Serial.available()) {
    delay(10);
  }
  String input = Serial.readStringUntil('\n');
  input.trim();
  if (input == "21e113") {
    Serial.println("21e113");
    oled.drawBitmap(0, 0, Deepak, 128, 64, WHITE);
    oled.display();
    delay(2000);
    oled.invertDisplay(1);
    delay(2000);
